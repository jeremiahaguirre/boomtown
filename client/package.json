{
  "name": "boomtown-client",
  "version": "1.0.0",
  "description": "Client-side code for RED Academy's Boomtown project.",
  "scripts": {
    "build": "react-scripts build && rm -rf ../server/public && mv build ../server/public",
    "precommit": "npm run lint --quiet",
    "eject": "react-scripts eject",
    "lint": "npm run prettier -- --list-different",
    "lint:fix": "npm run prettier -- --write",
    "prettier": "prettier '**/*.js'",
    "start": "react-scripts start",
    "test": "npm run lint && react-scripts test --env=jsdom"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "@material-ui/core": "1.4.3",
    "@material-ui/icons": "^1.1.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "final-form": "^4.18.5",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.10.1",
    "material-ui": "^0.20.2",
    "moment": "^2.24.0",
    "react": "^16.9.0",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.9.0",
    "react-final-form": "^3.7.0",
    "react-gravatar": "^2.6.3",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8"
  },
  "devDependencies": {
    "prettier": "1.13.7"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
